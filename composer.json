{
  "name": "san-kumar/laravel-crud",
  "description": "Laravel CRUD generator: Generate CRUD for any db table with the make:crud command.",
  "keywords": [
    "artisan",
    "laravel",
    "crud",
    "view",
    "templates",
    "blade"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "San Kumar",
      "email": "sanchitbh@gmail.com",
      "homepage": "https://dayandnight.in"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0 || ^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0 || ^10.0",
    "graham-campbell/testbench": "^5.6",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "San\\Crud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "San\\Crud\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "San\\Crud\\ServiceProvider"
      ]
    }
  }
}
